{
  "expo": {
    "name": "ChatApp",
    "slug": "ChatApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/chatlogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/chatlogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/chatlogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.stevenmcginnis.ChatApp"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": ["expo-localization"],
    "extra": {
      "eas": {
        "projectId": "251727fd-c633-4d55-b292-1cd03b6858f9"
      }
    }
  }
}
